{
  "name": "m-aldrin.space",
  "version": "0.0.1",
  "description": "source project for m-aldrin.space",
  "scripts": {
    "dev:eleventy": "npx @11ty/eleventy --serve",
    "dev:parcel": "parcel watch",
    "dev": "concurrently npm:dev:parcel npm:dev:eleventy",
    "build:eleventy": "npx @11ty/eleventy",
    "build:parcel": "parcel build",
    "buid": "concurrently npm:build*"
  },
  "author": "marcus aldrin",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-img": "^1.0.0",
    "@parcel/optimizer-css": "^2.3.2",
    "@parcel/transformer-css-experimental": "^2.3.2",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "parcel": "^2.3.2",
    "postcss": "^8.4.6"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "targets": {
    "default": {
      "source": [
        "src/js/main.js",
        "src/css/main.css"
      ],
      "distDir": "./_site"
    }
  },
  "@parcel/transformer-css": {
    "drafts": {
      "nesting": true,
      "customMedia": true
    }
  }
}