{
  "name": "m-aldrin.space",
  "version": "0.0.1",
  "description": "source project for m-aldrin.space",
  "scripts": {
    "dev:eleventy": "npx @11ty/eleventy --serve",
    "dev:parcel": "parcel watch src/main.css --dist-dir _site",
    "dev": "concurrently npm:dev:*",
    "build:eleventy": "npx @11ty/eleventy",
    "build:parcel": "parcel build src/main.css --dist-dir _site",
    "buid": "concurrently npm:build*"
  },
  "author": "marcus aldrin",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@parcel/optimizer-css": "^2.3.2",
    "@parcel/transformer-css-experimental": "^2.3.2",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "eleventy-postcss-extension": "^1.0.0",
    "parcel": "^2.3.2",
    "postcss-csso": "^6.0.0",
    "postcss-nested": "^5.0.6",
    "terser": "^5.10.0"
  },
  "@parcel/transformer-css": {
    "cssModules": true,
    "drafts": {
      "nesting": true,
      "customMedia": true
    },
    "pseudoClasses": {
      "focusVisible": "focus-ring"
    }
  }
}